<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.patient.PatientLogin">
   <children>
      <Text fill="#5eb9ff" layoutX="250.0" layoutY="121.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Log in">
         <font>
            <Font name="Century Gothic Bold" size="35.0" />
         </font>
      </Text>
      <TextField fx:id="patientName" layoutX="169.0" layoutY="149.0" prefHeight="28.0" prefWidth="262.0" promptText="Enter your name...." style="-fx-border-style: none; -fx-border-color: transparent;" />
      <TextField fx:id="patientID" layoutX="169.0" layoutY="185.0" prefHeight="28.0" prefWidth="262.0" promptText="Serial number (min 4 digit)" style="-fx-border-style: none; -fx-border-color: transparent;" />
      <Button fx:id="patientEnter" layoutX="270.0" layoutY="234.0" mnemonicParsing="false" onKeyPressed="#patientEnterActionKeyBoard" onMouseClicked="#patientEnterAction" style="-fx-background-color: #5b919e;" text="Enter" textFill="WHITE">
         <font>
            <Font name="Century Gothic Bold" size="16.0" />
         </font>
      </Button>
      <Text fx:id="newReg" fill="#5b919e" layoutX="169.0" layoutY="299.0" onMouseClicked="#newRegAction" strokeType="OUTSIDE" strokeWidth="0.0" text="New here? click me....">
         <font>
            <Font name="Century Gothic Bold" size="13.0" />
         </font>
      </Text>
      <Button fx:id="backToHome" layoutX="26.0" layoutY="31.0" mnemonicParsing="false" onMouseClicked="#backToHomeAction" style="-fx-background-color: #12284a;" text="&lt;&lt; Home" textFill="WHITE">
         <font>
            <Font name="Century Gothic Bold" size="11.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
