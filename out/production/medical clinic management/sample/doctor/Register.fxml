<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.doctor.Register">
   <children>
      <Label layoutX="194.0" layoutY="14.0" text="Registration Form" textFill="#5b919e">
         <font>
            <Font name="Century Gothic Bold" size="25.0" />
         </font>
      </Label>
      <Text layoutX="23.0" layoutY="92.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name :">
         <font>
            <Font name="Century Gothic Bold" size="20.0" />
         </font>
      </Text>
      <TextField fx:id="name" layoutX="130.0" layoutY="72.0" prefHeight="26.0" prefWidth="212.0" />
      <Text layoutX="21.0" layoutY="137.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Address :">
         <font>
            <Font name="Century Gothic Bold" size="20.0" />
         </font>
      </Text>
      <TextField fx:id="address" layoutX="130.0" layoutY="115.0" prefHeight="26.0" prefWidth="276.0" />
      <Text layoutX="349.0" layoutY="92.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Age :">
         <font>
            <Font name="Century Gothic Bold" size="20.0" />
         </font>
      </Text>
      <DatePicker fx:id="age" layoutX="406.0" layoutY="71.0" />
      <Text layoutX="21.0" layoutY="182.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Phone :">
         <font>
            <Font name="Century Gothic Bold" size="20.0" />
         </font>
      </Text>
      <TextField fx:id="phone" layoutX="130.0" layoutY="160.0" prefHeight="26.0" prefWidth="276.0" />
      <Text layoutX="21.0" layoutY="228.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Email :">
         <font>
            <Font name="Century Gothic Bold" size="20.0" />
         </font>
      </Text>
      <TextField fx:id="email" layoutX="130.0" layoutY="206.0" prefHeight="26.0" prefWidth="276.0" />
      <Text layoutX="21.0" layoutY="271.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password :">
         <font>
            <Font name="Century Gothic Bold" size="20.0" />
         </font>
      </Text>
      <TextField fx:id="password" layoutX="130.0" layoutY="249.0" prefHeight="26.0" prefWidth="276.0" />
      <Button layoutX="335.0" layoutY="302.0" mnemonicParsing="false" onAction="#submitAction" prefHeight="26.0" prefWidth="70.0" style="-fx-background-color: #5b919e;" text="Submit" textFill="WHITE">
         <font>
            <Font name="Century Gothic Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="180.0" layoutY="302.0" mnemonicParsing="false" onAction="#backToHome" style="-fx-background-color: #5b919e;" text="Back To Login" textFill="WHITE">
         <font>
            <Font name="Century Gothic Bold" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
